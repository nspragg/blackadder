    "json",
    "client"
  ],
  "author": "nspragg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bbc/blackadder/issues"
  },
  "homepage": "https://github.com/bbc/blackadder#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.4.0",
    "istanbul": "^0.4.2",
    "eslint": "^3.11.1",
    "mocha": "^3.0.2",
    "nock": "^9.0.4",
    "jsdoc": "^3.4.3",
    "sinon": "^1.15.3"
  },
  "jshintConfig": {
    "quotmark": "single",
